<div class="box tabular">
  <!--[form:project]-->

  <div class="splitcontent">
    <div class="splitcontentleft"><p><%= f.number_field :low, :required => true, :size => 60 %></p>
      <p><%= f.number_field :normal, :required => true, :size => 60 %></p>
      <p><%= f.number_field :high, :required => true, :size => 60 %></p>
      <p><%= f.number_field :urgent, :required => true, :size => 60 %></p>
      <p><%= f.number_field :immediate, :required => true, :size => 60 %></p></div>

    <div class="splitcontentright"><p><%= f.number_field :low_progress, :required => true, :size => 60 %></p>
      <p><%= f.number_field :normal_progress, :required => true, :size => 60 %></p>
      <p><%= f.number_field :high_progress, :required => true, :size => 60 %></p>
      <p><%= f.number_field :urgent_progress, :required => true, :size => 60 %></p>
      <p><%= f.number_field :immediate_progress, :required => true, :size => 60 %></p>
      <% f.hidden_field :project_id, value: params["project_id"] %></div>
  </div>
</div>
